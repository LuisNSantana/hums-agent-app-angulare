{
  "name": "agent-hums-app",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "serve:ssr:agent-hums-app": "node dist/agent-hums-app/server/server.mjs",
    "genkit:dev": "genkit start -- npm run serve:genkit",
    "genkit:serve": "tsx src/genkit-server.ts",
    "genkit:hybrid": "tsx src/genkit-hybrid-server.ts",
    "claude:test": "tsx src/test-server.ts",
    "claude:server": "tsx src/claude-server.ts",
    "claude:server:fixed": "tsx src/claude-server-fixed.ts",
    "serve:genkit": "tsx src/genkit-server.ts",
    "serve:hybrid": "tsx src/genkit-hybrid-server.ts",
    "serve:claude": "tsx src/claude-brave-test.ts",
    "serve:production": "tsx src/claude-server.ts",
    "dev:full": "concurrently \"npm run start\" \"npm run serve:genkit\"",
    "dev:hybrid": "concurrently \"npm run start\" \"npm run serve:hybrid\"",
    "dev:claude": "concurrently \"npm run start\" \"npm run claude:server:fixed\"",
    "build:genkit": "tsc src/genkit-server.ts --outDir dist/genkit",
    "genkit:ui": "genkit start"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^20.0.0",
    "@angular/cdk": "^20.0.1",
    "@angular/common": "^20.0.0",
    "@angular/compiler": "^20.0.0",
    "@angular/core": "^20.0.0",
    "@angular/forms": "^20.0.0",
    "@angular/material": "^20.0.1",
    "@angular/platform-browser": "^20.0.0",
    "@angular/platform-server": "^20.0.0",
    "@angular/router": "^20.0.0",
    "@angular/ssr": "^20.0.0",
    "@genkit-ai/core": "^1.11.1",
    "@genkit-ai/dotprompt": "^0.9.12",
    "@genkit-ai/express": "^1.11.1",
    "@genkit-ai/flow": "^0.5.17",
    "@supabase/supabase-js": "^2.49.8",
    "axios": "^1.9.0",
    "cheerio": "^1.0.0",
    "concurrently": "^9.1.2",
    "dotenv": "^16.5.0",
    "express": "^4.21.2",
    "genkit": "^1.11.1",
    "genkitx-anthropic": "^0.22.0",
    "genkitx-groq": "^0.22.0",
    "googleapis": "^149.0.0",
    "groq-sdk": "^0.23.0",
    "mammoth": "^1.9.1",
